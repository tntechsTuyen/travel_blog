<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/bg_dialog_main_menu_item"
    android:padding="16dp">


    <com.travel.app.common.view.icon.TextViewAwsRe
        android:id="@+id/twa_star_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ico_star"
        android:textColor="@color/color_green"
        android:textSize="@dimen/ico_big_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.travel.app.common.view.icon.TextViewAwsRe
        android:id="@+id/twa_star_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/ico_star"
        android:textColor="@color/color_green"
        android:textSize="@dimen/ico_big_large"
        app:layout_constraintStart_toEndOf="@+id/twa_star_1"
        app:layout_constraintTop_toTopOf="parent" />

    <com.travel.app.common.view.icon.TextViewAwsRe
        android:id="@+id/twa_star_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/ico_star"
        android:textColor="@color/color_green"
        android:textSize="@dimen/ico_big_large"
        app:layout_constraintStart_toEndOf="@+id/twa_star_2"
        app:layout_constraintTop_toTopOf="parent" />

    <com.travel.app.common.view.icon.TextViewAwsRe
        android:id="@+id/twa_star_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/ico_star"
        android:textColor="@color/color_green"
        android:textSize="@dimen/ico_big_large"
        app:layout_constraintStart_toEndOf="@+id/twa_star_3"
        app:layout_constraintTop_toTopOf="parent" />

    <com.travel.app.common.view.icon.TextViewAwsRe
        android:id="@+id/twa_star_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/ico_star"
        android:textColor="@color/color_green"
        android:textSize="@dimen/ico_big_large"
        app:layout_constraintStart_toEndOf="@+id/twa_star_4"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/btn_origin"
        android:text="Submit"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/twa_star_5"
        app:layout_constraintStart_toStartOf="@+id/twa_star_1"
        app:layout_constraintTop_toBottomOf="@+id/twa_star_3" />
</androidx.constraintlayout.widget.ConstraintLayout>